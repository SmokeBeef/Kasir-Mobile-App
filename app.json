{
  "expo": {
    "name": "kasir_tab",
    "slug": "kasirtab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/CashierIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Cashier.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/CashierIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.risha.kasir"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "LANDSCAPE"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e8f6caff-b2c5-4a3f-9b8b-09b9a49d5974"
      }
    }
  }
}
